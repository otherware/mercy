{
  "name": "mercy",
  "description": "Flow control library",
  "version": "0.0.1",
  "main": "lib/index.js",
  "repository": "git@github.com:otherware/mercy.git",
  "engines": {
    "node": ">=8.x.x",
    "npm": ">=5.x.x"
  },
  "keywords": [
    "async",
    "control",
    "flow"
  ],
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json",
    "linter": "lab -L",
    "reinstall": "rm -rf node_modules package-lock.json; npm i",

    "test-cov": "lab -L -v -t 100",
    "test-cov-html": "lab -L -v -t 100 -r html -o coverage.html",
    "test-cov-json": "lab -L -v -t 100 -r json -o coverage.json"
  },
  "dependencies": {
    "async": "2.x.x",
    "chalk": "2.x.x",

    "nock": "9.x.x",
    "purdy": "3.x.x",
    "urijs": "1.x.x",
    "wreck": "12.x.x"
  },
  "devDependencies": {
    "blipp": "3.x.x",
    "code": "5.x.x",
    "good-console": "6.x.x",
    "good-file": "6.x.x",
    "good-squeeze": "5.x.x",
    "good": "7.x.x",
    "hapi-swagger": "7.x.x",
    "hapi": "16.x.x",
